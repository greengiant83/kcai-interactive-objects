		  var socket = io();
		  socket.on('news', function (data) {
		    console.log(data);  
		  });
		</script>
		 function PowerButton()
		  {
		  	console.log("Power!!!!!");
		  	socket.emit('msg', "1");
		  }

		  function LeftChannel()
		  {
		  	console.log("Surf Left");
		  	socket.emit('msg', "2");
		  }

		  function RightChannel()
		  {
		  	console.log("Surf Right");
		  	socket.emit('msg', "3");
		  }

		  function VolumeUp()
		  {
		  	console.log("Turn down the volume!");
		  	socket.emit('msg', "4");
		  }

		  function VolumeDown()
		  {
		  	console.log("Turn up the volume!");
		  	socket.emit('msg', "5");
		  }
		</script>
